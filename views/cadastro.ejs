<%- include layout/header.ejs %>
<div class="container">
        <form class="form-horinzotal" action="cadastro" method="post" accept-charset="utf-8">
            <div class="form-group">
        	<label for="campoNome">Nome:</label>
        		<input class="form-control" id="campoNome" type="text" name="nome">	
            </div> 
            <div class="form-group">   
            	<label for="campoEmail">Email:</label>
            		<input class="form-control" id="campoEmail" type="email" name="email">
            </div>
            <div class="form-group">
        	   <label for="campoTel">Telefone:</label>
        		  <input type="text" class="form-control" id="campoTel" name="telefone">
            </div>    
            <div class="form-group">
                <label for="">Sexo</label>
                <div class="radio-inline">
            	    <label for="radioM">
            		  <input type="radio" id="radioM" name="sexo" value="m">Masculino
                    </label>
                </div>
                <div class="radio-inline">
                    <label for="radioF">
            		  <input type="radio" id="radioF" name="sexo" value="f">Feminino
                    </label>
                </div>
            </div>
        	<button class="btn btn-success" type="submit">Salvar</button>
            <a href="javascript:window.history.go(-1)" class="btn btn-danger">Voltar</a>
        </form>
</div>
<%-include layout/footer.ejs%>